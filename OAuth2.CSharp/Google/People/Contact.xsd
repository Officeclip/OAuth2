<xs:schema id="NewDataSet" xmlns="" xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
  <xs:element name="NewDataSet" msdata:IsDataSet="true" msdata:UseCurrentLocale="true">
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <!-- Schema to represent OfficeClip contacts. At this time only ten (10)
		user-defined fields can be communicated between OfficeClip and the web
		services clients. In near future this will be extended to unlimited 
		user defined fields -->
        <xs:element name="Contact">
          <xs:complexType>
            <xs:sequence>
              <!-- The serial id of this service type -->
              <xs:element name="sid" type="xs:string" minOccurs="0" />
              <!-- Determines if the column can be edited -->
              <xs:element name="isLock" type="xs:boolean" minOccurs="0" />
              <!-- Used by OfficeClip to pass specific information -->
              <xs:element name="tag" type="xs:string" minOccurs="0" />
              <!-- The Photo -->
              <xs:element name="photoBytes" type="xs:base64Binary" minOccurs="0" />
              <!-- The Notes -->
              <xs:element name="notes" type="xs:string" minOccurs="0" />
              <!-- The unique id of the contact in the OfficeClip database -->
              <xs:element name="contactId" type="xs:int" minOccurs="0" />
              <!-- The unique id of the organization in the OfficeClip database where the contact belongs -->
              <xs:element name="orgId" type="xs:int" minOccurs="0" />
              <!-- The first name of the contact -->
              <xs:element name="firstName" type="xs:string" minOccurs="0" />
              <!-- The last name of the contact -->
              <xs:element name="lastName" type="xs:string" minOccurs="0" />
              <!-- The salutation of the contact -->
              <xs:element name="salutation" type="xs:string" minOccurs="0" />
              <!-- The title of the contact -->
              <xs:element name="title" type="xs:string" minOccurs="0" />
              <!-- The company name of the contact -->
              <xs:element name="companyName" type="xs:string" minOccurs="0" />
              <!-- The work phone number for the contact -->
              <xs:element name="workPhone" type="xs:string" minOccurs="0" />
              <!-- The detaprtment where the contact works -->
              <xs:element name="department" type="xs:string" minOccurs="0" />
              <!-- The name of the person or position who this contact reports to -->
              <xs:element name="reportsTo" type="xs:string" minOccurs="0" />
              <!-- The email address for the contact -->
              <xs:element name="emailAddress" type="xs:string" minOccurs="0" />
              <!-- Shows whether the contact has desired to be opted out from the email -->
              <xs:element name="emailOptOut" type="xs:boolean" minOccurs="0" />
              <!-- The mobile phone number of the contact -->
              <xs:element name="mobilePhone" type="xs:string" minOccurs="0" />
              <!-- The alternate work phone number for the contact -->
              <xs:element name="alternateWorkPhone" type="xs:string" minOccurs="0" />
              <!-- The fax number of the contact -->
              <xs:element name="fax" type="xs:string" minOccurs="0" />
              <!-- The home phone number for the contact -->
              <xs:element name="homePhone" type="xs:string" minOccurs="0" />
              <!-- The currency type for the contact e.g. dollars, pounds etc. -->
              <xs:element name="currencyType" type="xs:string" minOccurs="0" />
              <!-- The birth date for the contact -->
              <xs:element name="birthDate" type="xs:dateTime" minOccurs="0" />
              <!-- The name of the assistant of the contact -->
              <xs:element name="assistantName" type="xs:string" minOccurs="0" />
              <!-- The phone number of the assistant of the contact -->
              <xs:element name="assistantPhone" type="xs:string" minOccurs="0" />
              <!-- The first line of the primary street address of the contact-->
              <xs:element name="address1" type="xs:string" minOccurs="0" />
              <!-- The second line of the primary street address of the contact-->
              <xs:element name="address2" type="xs:string" minOccurs="0" />
              <!-- the primary city of the contact -->
              <xs:element name="city" type="xs:string" minOccurs="0" />
              <!-- The primary state of the contact -->
              <xs:element name="state" type="xs:string" minOccurs="0" />
              <!-- The primary zip code for the contact -->
              <xs:element name="zip" type="xs:string" minOccurs="0" />
              <!-- The primary country for the contact -->
              <xs:element name="country" type="xs:string" minOccurs="0" />
              <!-- The first line of the other street address of the contact -->
              <xs:element name="otherAddress1" type="xs:string" minOccurs="0" />
              <!-- The second line of the other street address of the contact -->
              <xs:element name="otherAddress2" type="xs:string" minOccurs="0" />
              <!-- The other city of the contact -->
              <xs:element name="otherCity" type="xs:string" minOccurs="0" />
              <!-- The other state of the contact -->
              <xs:element name="otherState" type="xs:string" minOccurs="0" />
              <!-- The other zip code for the contact -->
              <xs:element name="otherZip" type="xs:string" minOccurs="0" />
              <!-- The other country for the contact -->
              <xs:element name="otherCountry" type="xs:string" minOccurs="0" />
              <!-- The source of the contact e.g. trade shows, cafe etc. -->
              <xs:element name="contactSource" type="xs:string" minOccurs="0" />
              <!-- The description for the contact-->
              <xs:element name="description" type="xs:string" minOccurs="0" />
              <!-- The name of the user that created the contact-->
              <xs:element name="createdUser" type="xs:string" minOccurs="0" />
              <!-- The created date for the contact-->
              <xs:element name="createdDate" type="xs:dateTime" minOccurs="0" />
              <!-- The name of the user that modified the contact -->
              <xs:element name="modifiedUser" type="xs:string" minOccurs="0" />
              <!-- The modified date for the contact -->
              <xs:element name="modifiedDate" type="xs:dateTime" minOccurs="0" />
              <!-- The name of the user who is assigned the current contact -->
              <xs:element name="owner" type="xs:string" minOccurs="0" />
              <!-- The website -->
              <xs:element name="webSite" type="xs:string" minOccurs="0" />
              <!-- The alternate email address -->
              <xs:element name="alternateEmailAddress" type="xs:string" minOccurs="0" />
              <!-- The work Parent Account for the contact -->
              <xs:element name="parentAccount" type="xs:string" minOccurs="0" />
              <!-- The first user defined field -->
              <xs:element name="udf1" type="xs:string" minOccurs="0" />
              <!-- The second user defined field -->
              <xs:element name="udf2" type="xs:string" minOccurs="0" />
              <!-- The third user defined field -->
              <xs:element name="udf3" type="xs:string" minOccurs="0" />
              <!-- The fourth user defined field -->
              <xs:element name="udf4" type="xs:string" minOccurs="0" />
              <!-- The fifth user defined field -->
              <xs:element name="udf5" type="xs:string" minOccurs="0" />
              <!-- The sixth user defined field -->
              <xs:element name="udf6" type="xs:string" minOccurs="0" />
              <!-- The seventh user defined field -->
              <xs:element name="udf7" type="xs:string" minOccurs="0" />
              <!-- The eighth user defined field -->
              <xs:element name="udf8" type="xs:string" minOccurs="0" />
              <!-- The nineth user defined field -->
              <xs:element name="udf9" type="xs:string" minOccurs="0" />
              <!-- The tenth user defined field -->
              <xs:element name="udf10" type="xs:string" minOccurs="0" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:choice>
    </xs:complexType>
  </xs:element>
</xs:schema>